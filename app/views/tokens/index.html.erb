<% @title = "アクセスキー一覧" %>

<table class="table">
  <thead>
    <tr>
      <th scope="col">#</th>
      <th scope="col">アクセスキー</th>
    </tr>
  </thead>
  <tbody>
    <% @tokens.each do |t| %>
      <tr>
        <th scope="row">
          <%= t.id %>
        </th>
        <td>
          <%= t.token %>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>
<div class="container">
  <%= form_tag "/tokens" do %>
    <%= submit_tag "新規作成", class: "btn btn-primary" %>
  <% end %>
</div>